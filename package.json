{
  "name": "tm-switcher",
  "version": "1.6.1",
  "main": "src/main.ts",
  "bin": "out/main.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "bundle": "npm run build && rm -rf bundle && pkg package.json --out-path bundle --targets latest-macos-x64,latest-win-x64,latest-linux-x64",
    "run": "rm -rf out && tsc && node out/main.js",
    "rb": "npm run build && npm run bundle && ./bundle/main-macos"
  },
  "dependencies": {
    "@types/inquirer": "^7.3.1",
    "atem-connection": "^3.0.1",
    "inquirer": "^7.3.3",
    "obs-websocket-js": "^5.0.1",
    "vex-tm-client": "^0.4.4"
  },
  "pkg": {
    "assets": [
      "node_modules/@julusian/freetype2/prebuilds/**/*",
      "node_modules/vex-tm-client/out/proto/**"
    ],
    "targets": [
      "latest-macos-x64",
      "latest-win-x64",
      "latest-linux-x64"
    ],
    "outputPath": "bundle"
  },
  "devDependencies": {
    "pkg": "^5.8.1",
    "typescript": "^4.8.3"
  }
}
